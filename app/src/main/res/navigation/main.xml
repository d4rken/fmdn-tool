<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/dashboard">
    <fragment
        android:id="@+id/dashboard"
        android:name="eu.darken.fmdn.main.ui.dashboard.DashboardFragment"
        tools:layout="@layout/main_fragment">
        <action
            android:id="@+id/action_dashboard_to_settingsContainerFragment"
            app:destination="@id/settingsContainerFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsContainerFragment"
        android:name="eu.darken.fmdn.main.ui.settings.SettingsFragment"
        android:label="SettingsContainerFragment" />

</navigation>